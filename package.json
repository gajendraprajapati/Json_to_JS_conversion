{
  "name": "brexunit",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc mocha --timeout 5000 -r ts-node/register test/scoringUnit.spec.ts --reporter mochawesome",
    "testCustom": "mocha --timeout 5000 -r ts-node/register test/customFieldAdapter.spec.ts --reporter mochawesome",
    "build": "tsc && cd dist && npm link && cd ..",
    "start": "node dist/index.js",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint -p tsconfig.json --fix"
  },
  "dependencies": {
    "@bugsnag/js": "^6.2.0",
    "@types/node-ipc": "^9.1.1",
    "@types/sinon": "^7.0.13",
    "artillery": "^1.6.0-28",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "escape-string-regexp": "^2.0.0",
    "express": "^4.17.1",
    "handlebars": "^4.1.2",
    "handlebars-helpers": "^0.10.0",
    "js-beautify": "^1.9.0-beta5",
    "lodash": "^4.17.19",
    "mongodb": "^3.2.6",
    "mongoose": "^5.5.11",
    "node-ipc": "^9.1.1",
    "perf_hooks": "0.0.1",
    "redis": "^2.8.0",
    "response-time": "^2.3.2",
    "shortid": "^2.2.14",
    "sinon": "^7.3.2",
    "underscore": "^1.9.1",
    "unescape": "1.0.1",
    "vm": "^0.1.0",
    "vm2": "^3.8.2"
  },
  "devDependencies": {
    "@glimpse/glimpse": "^0.22.15",
    "@types/assert": "^1.4.2",
    "@types/config": "0.0.34",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.9.4",
    "@types/sequelize": "^4.27.25",
    "assert": "^2.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "mocha": "^6.1.4",
    "mochawesome": "^4.0.1",
    "nyc": "^14.1.1",
    "reflect-metadata": "^0.1.12",
    "ts-node": "^7.0.1",
    "typescript": "3.0.3",
    "tslint": "^5.18.0"
  },
  "nyc": {
    "cache": false,
    "all": true,
    "sourceMap": true,
    "instrument": true,
    "exclude-after-remap": false,
    "extension": [
      ".ts"
    ],
    "check-coverage": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "include": [
      "src/**/*.ts",
      "test/**/*.spec.ts"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/**",
      "js/**",
      "builds/**",
      "config/**",
      "**/spec/**",
      "**/*.spec.ts",
      "**/node_modules/**",
      "gulpfile.js",
      "dummy-start.ts"
    ]
  },
  "author": "ratneshp@talentica.com",
  "copyright": "www.SoftCell.com"
}
